{
  "name": "websocket-server",
  "version": "1.0.0",
  "description": "WebSocket server for library and parking IoT integration",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc server.ts --outDir dist --module commonjs --target ES2020 --esModuleInterop",
    "start": "node dist/server.js",
    "dev": "nodemon --watch '*.ts' --exec ts-node server.ts",
    "ws:dev": "ts-node server.ts"
  },
  "keywords": [
    "websocket",
    "socket.io",
    "iot",
    "library",
    "parking"
  ],
  "author": "",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.2",
    "@types/node": "^24.9.1",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
